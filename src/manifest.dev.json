{
  "manifest_version": 2,
  "name": "Sendia - DEV",
  "description": "Send mass email campaigns with gmail!",
  "version": "0.11.11",
  "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----",
  "oauth2": {
    "client_id": "631473985497-vtklf9u12ts04qjs6bambp5g6n9en4a1.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/gmail.insert",
      "https://www.googleapis.com/auth/gmail.labels",
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/gmail.send",
      "https://www.googleapis.com/auth/spreadsheets.readonly",
      "https://www.googleapis.com/auth/drive.metadata.readonly"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "identity.email",
    "storage",
    "identity",
    "notifications",
    "http://localhost:3000/*",
    "https://sendia.us/*",
    "https://cdn.auth0.com/*",
    "https://sendiateam.auth0.com/*"
  ],
   "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["js/auth0-lock.js","js/inboxsdk.js","js/content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": { 
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}