{
  "manifest_version": 2,
  "name": "GMass Clone",
  "description": "A clone of GMass",
  "version": "0.3",
  "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----",
  "oauth2": {
    "client_id": "193054564435-piom2abbaujk94h9sebvlg83i20ej2ks.apps.googleusercontent.com",
    "scopes":[
      "https://mail.google.com/",
      "https://www.googleapis.com/auth/spreadsheets.readonly",
      "https://www.googleapis.com/auth/drive.metadata.readonly"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "identity",
    "notifications"
  ],
   "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["js/inboxsdk.js","js/content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": { 
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}